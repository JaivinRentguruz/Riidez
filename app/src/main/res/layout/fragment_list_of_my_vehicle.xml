<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="@color/selected_dot"
    android:layout_height="match_parent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/selectedlocation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="40dp"
            android:layout_marginBottom="20dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/back_ownerprofile"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="15dp"
                    android:paddingRight="12dp"
                    android:src="@drawable/ic_chevron_left" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/ubuntu_medium"
                    android:text="List of My Vehicles"
                    android:textColor="@color/TitleTextColor"
                    android:textSize="22dp" />

                <TextView
                    android:id="@+id/AddVehicle"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:layout_marginTop="4dp"
                    android:paddingRight="15dp"
                    android:fontFamily="@font/ubuntu_medium"
                    android:text="Add"
                    android:textAllCaps="true"
                    android:textColor="@color/TitleTextColor"
                    android:textSize="17dp" />


            </LinearLayout>
        </RelativeLayout>
    </LinearLayout>

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/selectedlocation"
        android:background="@drawable/curve_box"
        android:paddingLeft="3dp"
        android:paddingTop="12dp"
        android:paddingRight="3dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:layout_marginLeft="4dp"
            android:layout_marginRight="4dp"
            android:layout_height="match_parent">
           <RelativeLayout
               android:visibility="gone"
               android:id="@+id/rl_ListOfMyVehicle"
               android:layout_width="match_parent"
               android:layout_height="match_parent">
           </RelativeLayout>

            <LinearLayout
                android:id="@+id/lbl_listOfVeh"
                android:orientation="vertical"
                android:layout_margin="5dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="2dp"
                android:layout_marginTop="2dp"
                android:background="@drawable/top_curve_light_gray"
                android:orientation="vertical"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:weightSum="100"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="2dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/lbl_VehTypeName"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="75"
                        android:textSize="@dimen/txt_font_size"
                        android:padding="5dp"
                        android:layout_gravity="left"
                        android:textColor="@color/textColorHint"
                        android:fontFamily="@font/ubuntu_medium"
                        android:text="Economy"/>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_weight="25"
                        android:gravity="right"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content">
                        <LinearLayout
                            android:background="@drawable/aquamarine_bc"
                            android:layout_width="match_parent"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:layout_height="17dp">
                            <TextView
                                android:id="@+id/lbl_availableqty"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAlignment="gravity"
                                android:textColor="@color/screen_bg_color"
                                android:fontFamily="@font/ubuntu_medium"
                                android:text="5"
                                android:layout_margin="2dp"
                                android:textSize="8sp" />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Available"
                                android:textSize="8sp"
                                android:textAllCaps="true"
                                android:layout_margin="1dp"
                                android:textAlignment="gravity"
                                android:fontFamily="@font/ubuntu_medium"
                                android:textColor="@color/screen_bg_color">
                            </TextView>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:weightSum="20"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:layout_weight="14"
                        android:padding="5dp"
                        android:layout_marginLeft="7dp"
                        android:layout_marginBottom="4dp"
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:background="@drawable/curve_box_white"
                        android:layout_height="wrap_content">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:orientation="horizontal"
                            android:layout_height="wrap_content">
                            <TextView
                                android:id="@+id/lbl_vehName"
                                android:layout_width="wrap_content"
                                android:fontFamily="@font/ubuntu_medium"
                                android:textColor="@color/selected_dot"
                                android:textSize="@dimen/heading1_font_size"
                                android:text="Mitsubishi Mirage"
                                android:layout_height="wrap_content"/>
                            <TextView
                                android:layout_width="match_parent"
                                android:text=""
                                android:textColor="@color/textcolor"
                                android:textSize="@dimen/textsize_ten"
                                android:layout_height="wrap_content"/>
                        </LinearLayout>
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                            <ImageView
                                android:id="@+id/car_imgview"
                                android:src="@drawable/car_icon"
                                android:layout_marginTop="5dp"
                                android:layout_width="200dp"
                                android:layout_height="100dp"/>
                        </RelativeLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_weight="6"
                        android:layout_gravity="center"
                        android:gravity="center_horizontal"
                        android:padding="2dp"
                        android:layout_marginRight="5dp"
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <LinearLayout
                            android:orientation="horizontal"
                            android:padding="2dp"
                            android:layout_marginLeft="10dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:weightSum="8">
                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1">

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:src="@drawable/ic_gear_1"
                                    android:layout_height="wrap_content"/>
                            </LinearLayout>

                            <TextView
                                android:id="@+id/txt_automatic"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="4dp"
                                android:layout_weight="7"
                                android:fontFamily="@font/ubuntu_medium"
                                android:text="Automatic"
                                android:textSize="@dimen/textsize_ten"/>
                        </LinearLayout>
                        <LinearLayout
                            android:orientation="horizontal"
                            android:gravity="center"
                            android:padding="2dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="3dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:weightSum="8">
                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1">

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:src="@drawable/ic_car_seat_img"
                                    android:layout_height="wrap_content"/>
                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_weight="7"
                                android:orientation="horizontal"
                                android:layout_height="wrap_content">
                                <TextView
                                    android:id="@+id/lbl_seats"
                                    android:layout_marginLeft="4dp"
                                    android:textSize="@dimen/textsize_ten"
                                    android:fontFamily="@font/ubuntu_medium"
                                    android:text="5"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"/>
                                <TextView
                                    android:layout_marginLeft="4dp"
                                    android:textSize="@dimen/textsize_ten"
                                    android:fontFamily="@font/ubuntu_medium"
                                    android:text="Seats"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"/>
                            </LinearLayout>

                        </LinearLayout>
                        <LinearLayout
                            android:orientation="horizontal"
                            android:gravity="center"
                            android:padding="2dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="3dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:weightSum="8">
                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1">

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:src="@drawable/ac_icon"
                                    android:layout_height="wrap_content"/>
                            </LinearLayout>
                            <TextView
                                android:id="@+id/txt_ac"
                                android:layout_marginLeft="4dp"
                                android:layout_width="0dp"
                                android:layout_weight="7"
                                android:textSize="@dimen/textsize_ten"
                                android:fontFamily="@font/ubuntu_medium"
                                android:text="Air Condition"
                                android:layout_height="wrap_content"/>
                        </LinearLayout>
                        <LinearLayout
                            android:orientation="horizontal"
                            android:gravity="center"
                            android:padding="2dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="3dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:weightSum="8">
                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1">

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:src="@drawable/doors_icon"
                                    android:layout_height="wrap_content"/>
                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_weight="7"
                                android:orientation="horizontal"
                                android:layout_height="wrap_content">
                                <TextView
                                    android:id="@+id/lbl_doors"
                                    android:layout_marginLeft="4dp"
                                    android:textSize="@dimen/textsize_ten"
                                    android:fontFamily="@font/ubuntu_medium"
                                    android:text="5"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"/>
                                <TextView
                                    android:layout_marginLeft="4dp"
                                    android:textSize="@dimen/textsize_ten"
                                    android:fontFamily="@font/ubuntu_medium"
                                    android:text="Doors"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"/>
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout
                            android:orientation="horizontal"
                            android:gravity="center"
                            android:padding="2dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="3dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:weightSum="8">
                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1">

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:src="@drawable/bag_icon"
                                    android:layout_height="wrap_content"/>

                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_weight="7"
                                android:orientation="horizontal"
                                android:layout_height="wrap_content">
                                <TextView
                                    android:id="@+id/lbl_bags"
                                    android:layout_marginLeft="4dp"
                                    android:textSize="@dimen/textsize_ten"
                                    android:fontFamily="@font/ubuntu_medium"
                                    android:text="5"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"/>
                                <TextView
                                    android:layout_marginLeft="4dp"
                                    android:textSize="@dimen/textsize_ten"
                                    android:fontFamily="@font/ubuntu_medium"
                                    android:text="Bags"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"/>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:weightSum="4"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="2dp"
                android:layout_marginBottom="2dp"
                android:layout_height="35dp">
                <LinearLayout
                    android:layout_weight="2"
                    android:layout_width="0dp"
                    android:orientation="horizontal"
                    android:background="@drawable/green_bottom_curve"
                    android:layout_height="match_parent">
                    <TextView
                        android:layout_weight="1"
                        android:layout_width="wrap_content"
                        android:text="PLAY NOW"
                        android:textSize="@dimen/textsize_ten"
                        android:fontFamily="@font/ubuntu_medium"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textColor="@color/TitleTextColor"
                        android:layout_height="match_parent"/>
                    <LinearLayout
                        android:layout_weight="3"
                        android:gravity="right"
                        android:layout_gravity="right"
                        android:layout_marginRight="5dp"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:text="$ "
                            android:gravity="center"
                            android:fontFamily="@font/ubuntu_medium"
                            android:textColor="@color/TitleTextColor"
                            android:textSize="22dp"/>
                        <TextView
                            android:id="@+id/txt_pricenow"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:text="330.00"
                            android:gravity="center"
                            android:fontFamily="@font/ubuntu_medium"
                            android:textColor="@color/TitleTextColor"
                            android:textSize="22dp"/>
                    </LinearLayout>


                </LinearLayout>
                <LinearLayout
                    android:layout_weight="2"
                    android:layout_width="0dp"
                    android:orientation="horizontal"
                    android:background="@drawable/darkgreen_bottom_curve"
                    android:layout_height="match_parent">
                    <TextView
                        android:layout_weight="1"
                        android:layout_width="wrap_content"
                        android:text="PAY LATER"
                        android:textSize="@dimen/textsize_ten"
                        android:fontFamily="@font/ubuntu_medium"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textColor="@color/TitleTextColor"
                        android:layout_height="match_parent"/>
                    <LinearLayout
                        android:layout_weight="3"
                        android:gravity="right"
                        android:layout_gravity="right"
                        android:layout_marginRight="5dp"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:text="$ "
                            android:gravity="center"
                            android:layout_margin="2dp"
                            android:fontFamily="@font/ubuntu_medium"
                            android:textColor="@color/TitleTextColor"
                            android:textSize="22dp"/>
                        <TextView
                            android:id="@+id/txt_price_later"
                            android:layout_width="wrap_content"
                            android:text="380.00"
                            android:gravity="center"
                            android:fontFamily="@font/ubuntu_medium"
                            android:textColor="@color/TitleTextColor"
                            android:textSize="22dp"
                            android:layout_height="match_parent">
                        </TextView>
                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </androidx.core.widget.NestedScrollView>
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>